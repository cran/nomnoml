Package: nomnoml
Type: Package
Title: Sassy 'UML' Diagrams
Version: 0.3.0
Authors@R: c(
    person("Andrie", "de Vries", email = "apdevries@gmail.com", role = c("aut", "cre")),
    person("Javier", "Luraschi", email = "jluraschi@gmail.com", role = "aut"),
